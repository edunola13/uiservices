{% block beforehead %}{% endblock %}
{% block body %}
{% if config.inline == "no" %}
<div class="checkbox">
<label for="{{config.id}}">
{% if config.checked == "si" %}
<input type="checkbox" name="{{config.name}}" id="{{config.id}}" value="{{datos.value}}" checked="checked">
{% else %}
<input type="checkbox" name="{{config.name}}" id="{{config.id}}" value="{{datos.value}}">
{% endif %}
{{config.label}}
</label>
</div>
{% else %}
<label class="checkbox-inline" for="{{config.id}}">
{% if config.checked == "si" %}
<input type="checkbox" name="{{config.name}}" id="{{config.id}}" value="{{datos.value}}" checked="checked">
{% else %}
<input type="checkbox" name="{{config.name}}" id="{{config.id}}" value="{{datos.value}}">
{% endif %}
{{config.label}}
</label>
{% endif %}
{% endblock %}
{% block afterfoot %}{% endblock %}